{
  "name": "lacona-etym",
  "version": "0.1.0",
  "description": "Lacona Addon to Find the etymology of an English word in the Online Etymology Dictionary.",
  "main": "lib/index.js",
  "lacona": {
    "title": "Etymology Dictionary",
    "description": "Find the etymology of an English word in the Online Etymology Dictionary.",
    "config": "config.json"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "lacona",
    "lacona-addon"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4hines/lacona-etym.git"
  },
  "dependencies": {
    "lacona-phrases": "^1.0.0",
    "lacona-api": "^1.0.0",
    "elliptical": "^1.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-cli": "^6.0.0",
    "rimraf": "^2.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
